{
  "name": "ns_boilerplate",
  "version": "0.0.1",
  "description": "A boilerplate for creating new projects",
  "main": "index.js",
  "repository": "https://github.com/alvinwhite/ns_boilerplate",
  "author": "Alvvi ",
  "license": "MIT",
  "scripts": {
    "dev": "set NODE_ENV=development && node_modules/.bin/webpack-dev-server --host 0.0.0.0",
